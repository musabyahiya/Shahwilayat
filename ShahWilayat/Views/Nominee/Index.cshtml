
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterPage.cshtml";
}


<div class="content-header row">
    <div class="content-header-left col-md-6 col-12 mb-2">
        <h3 class="content-header-title">Nominee</h3>
    </div>

</div>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <button type="button" class="btn btn-info" data-toggle="modal" data-target="#CreateNominee">
                    Add New Nominee
                </button>
            </div>
        </div>
    </div>
</div>
<!-- Both borders end-->
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <fieldset>
                    <div class="input-group">
                        <input type="text" class="form-control txtSearch" placeholder="Search..." aria-describedby="basic-addon4">
                        <div class="input-group-append">
                            <span class="input-group-text" id="basic-addon4"><i class="la la-search"></i></span>
                        </div>
                    </div>
                </fieldset>

            </div>
            <div class="card-content">

                <div class="table-responsive">
                    <table class="table table-bordered mb-0 tblNominee">
                        <thead>
                            <tr>
                                <th nowrap>Sr No.</th>
                                <th>Membership No</th>
                                <th>Member</th>
                                <th>Nominee</th>
                                <th>Relation</th>
                                <th>Member Relation</th>
                                <th>Father/ Husband Name</th>
                                <th>Action</th>

                            </tr>
                        </thead>
                        <tbody class="NomineeListing"></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>




@*Create Modal Start*@
@*Create Modal Start*@
<div class="modal fade text-left" id="CreateNominee" role="dialog" aria-labelledby="myModalLabel16" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel16">Create New Nominee</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-body frmNominee">
                    <h4 class="form-section"><i class="ft-user"></i> Personal Info</h4>
                    <hr>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlMember">Member</label>
                                <select id="ddlMember" class="form-control ddlMember select2 mySelect" style="width:100%"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlTitle">Title</label>
                                <select id="ddlTitle" class="form-control ddlTitle"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlRelation">Relation</label>
                                <select id="ddlRelation" class="form-control ddlRelation"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtFirstName">First Name</label>
                                <input type="text" id="txtFirstName" class="form-control txtFirstName" placeholder="Enter first name">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtLastName">Last Name</label>
                                <input type="text" id="txtLastName" class="form-control txtLastName" placeholder="Enter last name">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlMemberRelation">Member Relation</label>
                                <select id="ddlMemberRelation" class="form-control ddlMemberRelation"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtFatherName">Father Name</label>
                                <input type="text" id="txtFatherName" class="form-control txtFatherName" placeholder="Enter father name">
                            </div>
                        </div>
                     
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtDob">Date of Birth</label>
                                <input type="text" id="txtDob" class="form-control DatePicker txtDob" placeholder="yyyy-mm-dd">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtBirthPlace">Place of Birth</label>
                                <input type="text" id="txtBirthPlace" class="form-control txtBirthPlace" placeholder="Enter birth place">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtCellNo">Cell No</label>
                                <input type="text" id="txtCellNo" class="form-control txtCellNo numbericOnly" placeholder="Enter cell number">
                            </div>
                        </div>
                   
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtCNIC">CNIC</label>
                                <input type="text" id="txtCNIC" class="form-control txtCNIC " placeholder="Enter CNIC without dash">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtAddress">Permanent Address</label>
                                <textarea class="form-control txtAddress" id="txtAddress" rows="1" placeholder="Enter permanent address"></textarea>
                            </div>
                        </div>
                    </div>
                    <h4 class="form-section"><i class="ft-paperclip"></i> Attachments</h4>
                    <hr>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach CNIC Front</span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtCnicFront" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach CNIC Back</span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtCnicBack" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach Recent Profile Picture</span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtProfileFile" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach Birth Certificate</span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtBirthCertificate" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach Hereship Certificate</span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtHereshipCertificate" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach FRC </span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtFrcFile" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach Guardian Certificate </span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtGuardianCertificate" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="fileinput fileinput-new" data-provides="fileinput">

                                <span class="btn btn-block btn-outline btn-info btn-file">
                                    <span class="fileinput-new">Attach Death Certificate </span>
                                    <i class="fa fa-upload"></i>&nbsp;&nbsp;
                                    <span class="fileinput-exists">Change</span><input type="file" name="..." class="txtDeathCertificate" accept="image/*" />
                                </span>
                                <span class="fileinput-filename"></span>
                                <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">×</a>
                            </div>
                        </div>

                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn grey btn-outline-info" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-outline-dark btnSaveChanges">Save changes</button>
            </div>
        </div>
    </div>
</div>
@*Create Modal End*@



@*Create Modal End*@


@*Edit Modal Start*@
<div class="modal fade text-left" id="EditNominee" role="dialog" aria-labelledby="myModalLabel16" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel16">Edit Nominee</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-body frmNominee_upd">
                    <h4 class="form-section"><i class="la la-file-text"></i> Nominee Details</h4>
                    <hr>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlMember_upd">Member</label>
                                <select id="ddlMember_upd" class="form-control ddlMember_upd select2 mySelect" disabled style="width: 100%"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlPlot_upd">Plot No</label>
                                <select id="ddlPlot_upd" class="form-control ddlPlot_upd select2 mySelect" disabled style="width: 100%"></select>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtNomineeOrderNo">Nominee No</label>
                                <input type="text" id="txtNomineeOrderNo_upd" class="form-control txtNomineeOrderNo_upd" placeholder="Enter Nominee no">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtNomineeOrderDate_upd">Nominee Date</label>
                                <input type="text" id="txtNomineeOrderDate_upd" class="form-control DatePicker txtNomineeOrderDate_upd" placeholder="yyyy-mm-dd">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtProvisionalNomineeNo_upd">Provisional Nominee No</label>
                                <input type="text" id="txtProvisionalNomineeNo_upd" class="form-control txtProvisionalNomineeNo_upd " placeholder="Enter provisional Nominee no">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtProvisionalNomineeDate_upd">Provisional Nominee Date</label>
                                <input type="text" id="txtProvisionalNomineeDate_upd" class="form-control DatePicker txtProvisionalNomineeDate_upd " placeholder="yyyy-mm-dd">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtShareCertificateNo_upd">Share Certificate No</label>
                                <input type="text" id="txtShareCertificateNo_upd" class="form-control txtShareCertificateNo_upd " placeholder="Enter share certificate no">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtShareCertificateDate">Share Certificate Date</label>
                                <input type="text" id="txtShareCertificateDate_upd" class="form-control DatePicker txtShareCertificateDate_upd " placeholder="yyyy-mm-dd">
                            </div>
                        </div>

                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn grey btn-outline-info" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-outline-dark btnUpdateChanges">Save changes</button>
            </div>
        </div>
    </div>
</div>

@*Edit Modal Start*@

@section Scripts
{
    <script type="text/x-jQuery-tmpl" id="NomineeListing">
        <tr class="trNominee">

            <input type="hidden" class="hdnNomineeId" value="${NomineeId}" />
            <input type="hidden" class="hdnMemberId" value="${MemberId}" />
            <input type="hidden" class="hdnTitleId" value="${TitleId}" />
            <input type="hidden" class="hdnMemberRelationId" value="${MemberRelationId}" />
            <input type="hidden" class="hdnRelationId" value="${RelationId}" />
            <input type="hidden" class="hdnFirstName" value="${FirstName}" />
            <input type="hidden" class="hdnLastName" value="${LastName}" />
            <input type="hidden" class="hdnFatherName" value="${FatherName}" />
            <input type="hidden" class="hdnDob" value="${ToJavaScriptDate(Dob)}" />
            <input type="hidden" class="hdnBirthPlace" value="${BirthPlace}" />
            <input type="hidden" class="hdnCNIC" value="${CNIC}" />
            <input type="hidden" class="hdnCellNo" value="${CellNo}" />
            <input type="hidden" class="hdnAddress" value="${Address}" />
            <input type="hidden" class="hdnCnicFront" value="${CnicFront}" />
            <input type="hidden" class="hdnCnicBack" value="${CnicBack}" />
            <input type="hidden" class="hdnFrcFile" value="${FrcFile}" />
            <input type="hidden" class="hdnHereshipCertificate" value="${HereshipCertificate}" />
            <input type="hidden" class="hdnGuardianCertificate" value="${GuardianCertificate}" />
            <input type="hidden" class="hdnDeathCertificate" value="${DeathCertificate}" />

            <td></td>
            <td class="project-title tdMemberShipNo" nowrap>${MembershipNo}</td>
            <td class="project-title tdMemberName" nowrap>${MemberName}</td>
            <td class="project-title tdName" nowrap>${FirstName + ' ' + LastName}</td>
            <td class="project-title tdRelation" nowrap>${Relation}</td>
            <td class="project-title tdMemberRelation" nowrap>${MemberRelation}</td>
            <td class="project-title tdFatherName" nowrap>${FatherName}</td>


            <td class="project-title">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#EditNominee" onclick="EditNominee(this)" data-toggle="tooltip" data-placement="top" data-original-title="Edit Nominee"><i class="la la-pencil-square success"></i></a>
                <a href="javascript:void(0)" data-toggle="modal" data-target="#DeleteNominee" onclick="EditNominee(this)" data-toggle="tooltip" data-placement="top" data-original-title="Delete Nominee"><i class="la la-trash danger"></i></a>

             </td>


        </tr>
    </script>

    <script src="~/Scripts/assets/js/PagesJS/Nominee.js"></script>


}




