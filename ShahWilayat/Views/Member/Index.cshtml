@section Scripts
{
    <script type="text/x-jQuery-tmpl" id="MemberListing">
        <tr class="trMembers">

            <input type="hidden" class="hdnMemberId" value="${MemberId}" />
            <input type="hidden" class="hdnTitleId" value="${TitleId}" />
            <input type="hidden" class="hdnMemberRelationId" value="${MemberRelationId}" />
            <input type="hidden" class="hdnGenderId" value="${GenderId}" />
            <input type="hidden" class="hdnCountryId" value="${CountryId}" />
            <input type="hidden" class="hdnProvinceId" value="${ProvinceId}" />
            <input type="hidden" class="hdnCityId" value="${CityId}" />
            <input type="hidden" class="hdnFirstName" value="${FirstName}" />
            <input type="hidden" class="hdnLastName" value="${LastName}" />
            <input type="hidden" class="hdnFatherName" value="${FatherName}" />
            <input type="hidden" class="hdnCellNo" value="${CellNo}" />
            <input type="hidden" class="hdnLandline" value="${Landline}" />
            <input type="hidden" class="hdnWhatsApp" value="${WhatsApp}" />
            <input type="hidden" class="hdnOfficePhone" value="${OfficePhone}" />
            <input type="hidden" class="hdnEmail" value="${Email}" />
            <input type="hidden" class="hdnDob" value="${ToJavaScriptDate(Dob)}" />
            <input type="hidden" class="hdnBirthPlace" value="${BirthPlace}" />
            <input type="hidden" class="hdnCNIC" value="${CNIC}" />
            <input type="hidden" class="hdnCnicExpiryDate" value="${ToJavaScriptDate(CnicExpiryDate)}" />
            <input type="hidden" class="hdnBloodGroup" value="${BloodGroup}" />
            <input type="hidden" class="hdnPostalCode" value="${PostalCode}" />
            <input type="hidden" class="hdnPermanentAddress" value="${PermanentAddress}" />
            <input type="hidden" class="hdnOccupation" value="${Occupation}" />
            <input type="hidden" class="hdnMembershipNo" value="${MembershipNo}" />
            <input type="hidden" class="hdnMembershipDate" value="${ToJavaScriptDate(MembershipDate)}" />
            <input type="hidden" class="hdnReferenceNo" value="${ReferenceNo}" />
            <input type="hidden" class="hdnFolioNo" value="${FolioNo}" />
            <input type="hidden" class="hdnCnicFrontFile" value="../Uploads/${JSON.parse(CnicFrontFile)[0]}" />
            <input type="hidden" class="hdnCnicBackFile" value="../Uploads/${JSON.parse(CnicBackFile)[0]}" />
            <input type="hidden" class="hdnProfileFile" value="../Uploads/${JSON.parse(ProfileFile)[0]}" />

            <td align="center"></td>
            <td class="project-title tdMembershipNo">${MembershipNo}</td>
