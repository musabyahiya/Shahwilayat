
@{
    ViewBag.Title = "Member - Shah Wilayat Cooperative Housing Society";
    Layout = "~/Views/Shared/_MasterPage.cshtml";
}

<div class="content-header row">
    <div class="content-header-left col-md-6 col-12 mb-2">
        <h3 class="content-header-title">Member</h3>
    </div>

</div>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <button type="button" class="btn btn-info" data-toggle="modal" data-target="#CreateMember">
                    Add New Member
                </button>
            </div>
        </div>
    </div>
</div>
<!-- Both borders end-->
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <fieldset>
                    <div class="input-group">
                        <input type="text" class="form-control txtSearch" placeholder="Search..." aria-describedby="basic-addon4">
                        <div class="input-group-append">
                            <span class="input-group-text" id="basic-addon4"><i class="la la-search"></i></span>
                        </div>
                    </div>
                </fieldset>

            </div>
            <div class="card-content">

                <div class="table-responsive">
                    <table class="table table-bordered mb-0 tblMember">
                        <thead>
                            <tr>
                                <th style="text-align:center">Sr No.</th>
                                <th>Membership No</th>
                                <th>Date</th>
                                <th>Name</th>
                                <th>Cell No</th>
                                <th>Status</th>
                                <th class="align-middle">Action</th>
                            </tr>
                        </thead>
                        <tbody class="MemberListing"></tbody>
                        <tfoot>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@*Create Modal Start*@
<div class="modal fade text-left" id="CreateMember" role="dialog" aria-labelledby="myModalLabel16" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel16">Create New Member</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-body frmMember">
                    <h4 class="form-section"><i class="ft-user"></i> Personal Info</h4>
                    <hr>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlTitle">Title</label>
                                <select id="ddlTitle" class="form-control ddlTitle"></select>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtFirstName">First Name</label>
                                <input type="text" id="txtFirstName" class="form-control txtFirstName" placeholder="Enter first name">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtLastName">Last Name</label>
                                <input type="text" id="txtLastName" class="form-control txtLastName" placeholder="Enter last name">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlMemberRelation">Relation</label>
                                <select id="ddlMemberRelation" class="form-control ddlMemberRelation"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtFatherName">Father Name</label>
                                <input type="text" id="txtFatherName" class="form-control txtFatherName" placeholder="Enter father name">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="ddlGender">Gender</label>
                                <select id="ddlGender" class="form-control ddlGender"></select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtDob">Date of Birth</label>
                                <input type="text" id="txtDob" class="form-control DatePicker txtDob" placeholder="yyyy-mm-dd">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtBirthPlace">Place of Birth</label>
                                <input type="text" id="txtBirthPlace" class="form-control txtBirthPlace" placeholder="Enter birth place">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtCellNo">Cell No</label>
                                <input type="text" id="txtCellNo" class="form-control txtCellNo numbericOnly" placeholder="Enter cell number">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtWhatsApp">WhatsApp No</label>
                                <input type="text" id="txtWhatsApp" class="form-control txtWhatsApp numericOnly notrequired" placeholder="Enter whatsapp number">
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtLandline">Landline</label>
                                <input type="text" id="txtLandline" class="form-control txtLandline numericOnly notrequired" placeholder="Enter landline number">
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtOfficePhone">Office Phone</label>
                                <input type="text" id="txtOfficePhone" class="form-control txtOfficePhone numericOnly notrequired" placeholder="Enter OfficePhone number">
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtEmail">Email</label>
                                <input type="text" id="txtEmail" class="form-control txtEmail  notrequired" placeholder="Enter email">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label for="txtCNIC">CNIC</label>
                                <input type="text" id="txtCNIC" class="form-control txtCNIC " placeholder="Enter CNIC without dash">
                            </div>
                        </div>

                        <div class="col-md-3">
